# Google Sheets Configuration (Product Hunt specific)
# Path to your service account JSON key or the JSON string itself
PH_GDRIVE_API_KEY_JSON='{"type": "service_account", ...}'

# Target Sheet ID (from the URL: https://docs.google.com/spreadsheets/d/<SHEET_ID>/edit)
PH_GSHEET_ID=your-sheet-id-here

# Target Sheet (non-sensitive, can keep generic names)
GSHEET_NAME=Product Hunt Rankings
GSHEET_TAB=Weekly Top 10

# Logging
LOG_LEVEL=INFO
